<h2>Update <em><%= @cabinet.name %></em> Cabinet</h1>

<form method="POST" action="/cabinets/<%= @cabinet.slug %>">
    <input type="hidden" id="hidden" name="_method" value="patch">

    <label>Edit Cabinet Name: </label>
    <input type="text" id="name" name="cabinet[name]">

    <input type="submit" value="Edit">
</form>